{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/content/(.*)",
      "dest": "/content/$1"
    },
    {
      "src": "^/day_share2/(.*)",
      "dest": "/day_share2/$1"
    },
    {
      "src": "^/thumbnails/(.*)",
      "dest": "/thumbnails/$1"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "^/favicon-96x96.png",
      "dest": "/favicon-96x96.png"
    },
    {
      "src": "^/apple-touch-icon.png",
      "dest": "/apple-touch-icon.png"
    },
    {
      "src": "^/web-app-manifest-192x192.png",
      "dest": "/web-app-manifest-192x192.png"
    },
    {
      "src": "^/web-app-manifest-512x512.png",
      "dest": "/web-app-manifest-512x512.png"
    },
    {
      "src": "^/site.webmanifest",
      "dest": "/site.webmanifest"
    },
    {
      "src": "^/background.jpg",
      "dest": "/background.jpg"
    },
    {
      "src": "^/cardboard.jpg",
      "dest": "/cardboard.jpg"
    },
    {
      "src": "^/olio-christmas.jpg",
      "dest": "/olio-christmas.jpg"
    },
    {
      "src": "^/vite.svg",
      "dest": "/vite.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
